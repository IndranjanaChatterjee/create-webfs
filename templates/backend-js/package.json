{
    "name": "web-fs-backend-js",
    "version": "1.0.0",
    "description": "Backend for web-fs project (JavaScript)",
    "main": "server.js",
    "type": "commonjs",
    "scripts": {
      "start": "node server.js",
      "start:local": "dotenv -e .env.local -- node server.js",
      "start:dev": "dotenv -e .env.local -- nodemon server.js",
      "start:test": "dotenv -e .env.test -- node server.js",
      "start:prod": "dotenv -e .env.prod -- node server.js",
      "prisma:generate": "prisma generate",
      "prisma:migrate:dev": "prisma migrate dev",
      "prisma:studio": "prisma studio"
    },
    "dependencies": {
      "@prisma/client": "^5.14.0", 
      "cors": "^2.8.5", 
      "dotenv": "^16.4.5",
      "express": "^4.19.2"
    },
    "devDependencies": {
      "dotenv-cli": "^7.4.2",
      "nodemon": "^3.1.0",
      "prisma": "^5.14.0" 
    }
  }